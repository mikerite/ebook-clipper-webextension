{
  "name": "mikerite-ebook-clipper",
  "version": "0.9.0",
  "description": "A Firefox extension for creating eBooks from page clippings",
  "main": "web-ext-config.js",
  "scripts": {
    "format": "prettier --list-different --no-config **/*.js",
    "lint": "eslint . --ignore-pattern 'src/lib/*'",
    "start": "web-ext run --browser-console --start-url \"https://en.wikisource.org/wiki/The_Mysterious_Affair_at_Styles/Chapter_3\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Michael Wright",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^9.1.1",
    "prettier": "^2.0.5",
    "web-ext": "^4.3"
  }
}
